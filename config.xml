<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="at.ac.meduniwien.pallcare" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>PallCare</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="patrick.huber@meduniwien.ac.at" href="http://www.pallcare.at/">
    Patrick Huber
  </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>

  <platform name="ios">
    <!-- iOS 7.0+ -->
    <!-- iPhone / iPod Touch -->
    <icon src="icons/ios/Icon-60@2x.png" width="120" height="120" />
    <icon src="icons/ios/Icon-60@3x.png" width="180" height="180" />

    <!-- iPad -->
    <icon src="icons/ios/Icon-76.png" width="76" height="76" />
    <icon src="icons/ios/Icon-76@2x.png" width="152" height="152" />

    <!-- iOS 6.1 -->
    <!-- Spotlight Icon -->
    <icon src="icons/ios/Icon-Small-40.png" width="40" height="40" />
    <icon src="icons/ios/Icon-Small-40@2x.png" width="80" height="80" />

    <!-- iPhone / iPod Touch -->
    <icon src="icons/ios/Icon.png" width="57" height="57" />
    <icon src="icons/ios/Icon@2x.png" width="114" height="114" />

    <!-- iPad -->
    <icon src="icons/ios/Icon-72.png" width="72" height="72" />
    <icon src="icons/ios/Icon-72@2x.png" width="144" height="144" />

    <!-- iPhone Spotlight and Settings Icon -->
    <icon src="icons/ios/Icon-Small.png" width="29" height="29" />
    <icon src="icons/ios/Icon-Small@2x.png" width="58" height="58" />

    <!-- iPad Spotlight and Settings Icon -->
    <icon src="icons/ios/Icon-Small-50.png" width="50" height="50" />
    <icon src="icons/ios/Icon-Small-50@2x.png" width="100" height="100" />
  </platform>
</widget>